<problem_tmpl name="PR1065" width="600">
  <p>Round the circled number on the number line to the nearest ten.</p>
  <p><img src="https://res.cloudinary.com/ansr/image/upload/v1563182713/Tutor/SH1002.svg" width="400px" alt="Number line for nearest ten"/></p>
  <group name="PR1065" type="FIB">
    <p><fib name="fib1" /></p>
    <solutions>
      <solution><cond><fib_ref name="fib1"/>==60</cond></solution>
    </solutions>
    <tutelage_ref name="TU1065"></tutelage_ref>
  </group>
  <group name="PR1065" type="MCQ">
    <choice name="C1">50</choice>
    <choice name="C2">55</choice>
    <choice name="C3">60</choice>
    <choice name="C4">56</choice>
    <solutions>
      <solution>
        <repeat val="4" index="i">
          <cond>
            <choice_ref name="C$i+1$"/>==$(i)==(2)$
          </cond>
        </repeat>
      </solution>
    </solutions>
    <tutelage_ref name="TU1065M"></tutelage_ref>
  </group>
</problem_tmpl>

<problem_tmpl name="PR1066" width="200">
  <params>
    <param name="A" type="int"/>
    <param name="B" type="int"/>
    <constraints>
      <cond>1 &#60; A &#60; 9</cond>
      <cond>5 &#60; B &#60; 9</cond>
    </constraints>
  </params>
  <p>A baker baked $(A*10)+B$ loaves of bread in a day. Round the number of loaves of bread to the nearest ten.</p>
  <group name="PR1066" type="FIB">
    <p><fib name="fib1" /> loaves of bread</p>
    <solutions>
      <solution><cond><fib_ref name="fib1"/>==$((A+1)*10)$</cond></solution>
    </solutions>
    <tutelage_ref name="TU1066">
      <bind name="A" val="A"/>
      <bind name="B" val="B"/>
    </tutelage_ref>
  </group>
  <group name="PR1066" type="MCQ">
    <choice name="C1">$(A*10)+B$ loaves of bread</choice>
    <choice name="C2">$((A+1)*10)$ loaves of bread</choice>
    <choice name="C3">$(A*10)$ loaves of bread</choice>
    <choice name="C4">$(A*10)+5$ loaves of bread</choice>
    <solutions>
      <solution>
        <repeat val="4" index="i">
          <cond>
            <choice_ref name="C$i+1$"/>==$(i)==(1)$
          </cond>
        </repeat>
      </solution>
    </solutions>
    <tutelage_ref name="TU1066M"></tutelage_ref>
  </group>
</problem_tmpl>

