<problem_tmpl name="PR1070" width="300">
  <params>
    <param name="A" type="int"/>
    <param name="B" type="int"/>
    <param name="C" type="int"/>
    <constraints>
      <cond>0 &#60; A &#60; 9</cond>
      <cond>1 &#60; C &#60; 9</cond>
      <cond>B &#61;&#61; 5</cond>
    </constraints>
  </params>
  <p>Students in a class collected $$$(A*100)+(B*10)+(C)$ for the local animal shelter. Round the amount collected to the nearest hundred.</p>
  <group name="PR1070" type="FIB">
    <p>$$ <fib name="fib1" /></p>
    <solutions>
      <solution><cond><fib_ref name="fib1"/>==$(A+1)*100$</cond></solution>
    </solutions>
    <tutelage_ref name="TU1070">
      <bind name="A" val="A"/>
      <bind name="B" val="B"/>
      <bind name="C" val="C"/>
    </tutelage_ref>
  </group>
  <group name="PR1070" type="MCQ">
    <choice name="C1">$$$(A+1)*100$</choice>
    <choice name="C2">$$$A*100$</choice>
    <choice name="C3">$$$(Math.round(A*10+B+C/10))*10$</choice>
    <choice name="C4">$$$(A*100+B*10+C)$</choice>
    <solutions>
      <solution>
        <repeat val="4" index="i">
          <cond>
            <choice_ref name="C$i+1$"/>==$(i)==(0)$
          </cond>
        </repeat>
      </solution>
    </solutions>
    <tutelage_ref name="TU1070M"></tutelage_ref>
  </group>
</problem_tmpl>

