    </solutions>
    <tutelage_ref name="TU1111">
      <bind name="A" val="A"/>
      <bind name="B" val="B"/>
      <bind name="C" val="C"/>
      <bind name="D" val="D"/>
      <bind name="E" val="E"/>
      <bind name="F" val="F"/>
    </tutelage_ref>
