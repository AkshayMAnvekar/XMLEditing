    <choice name="C2">$(Math.round(A/10) - Math.round(B/10))*10$ bottles</choice>
    <choice name="C3">$(A-B)$ bottles</choice>
    <solutions>
      <solution>
        <repeat index="i" val="3">
          <cond><choice_ref name="C$i+1$"/>==$(i)==(1)$</cond>
        </repeat>
      </solution>
    </solutions>
    <tutelage_ref name="TU1108M"></tutelage_ref>
  </group>
  <group name="PR1108" type="FIB">
    <p>He has about <fib type="int" name="fib1"/> bottles left.</p>
    <solutions>
      <solution>
        <cond>
          <fib_ref name="fib1"/>==$Math.round(A/10)*10 - Math.round(B/10)*10$
        </cond>
      </solution>
    </solutions>
    <tutelage_ref name="TU1108">
      <bind name="A" val="A"/>
      <bind name="B" val="B"/>
    </tutelage_ref>
  </group>
</problem_tmpl>
