        <cond><fib_ref name="fib1"/>==$((Math.round(A/100)+Math.round(B/100))*100)$</cond>
      </solution>
    </solutions>
    <tutelage_ref name="TU1138G1">
      <bind name="A" val="A"/>
      <bind name="B" val="B"/>
    </tutelage_ref>
  </group>
  <group>
    <p>Exact: <fib type="int" name="fib2"/></p>
    <solutions>
      <solution>
        <cond><fib_ref name="fib2"/>==$(A+B)$</cond>
      </solution>
    </solutions>
    <tutelage_ref name="TU1138G2">
      <bind name="A" val="A"/>
      <bind name="B" val="B"/>
    </tutelage_ref>
  </group>
</problem_tmpl>
