    <choice name="C2">$Math.round(A/10)*10+Math.round(B/10)*10+10$</choice>
    <choice name="C3">$Math.round(A/10)*10+Math.round(B/10)*10-10$</choice>
    <choice name="C4">$Math.round(A/10)*10+Math.round(B/10)*10+20$</choice>
    <solutions>
