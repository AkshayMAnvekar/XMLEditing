          </repeat>
        </div>
      </solution>
    </solutions>
    <tutelage_ref name="TU1082M"></tutelage_ref>
  </group>
  <group name="PR1082" type="FIB">
    <p>$A$ + $B$ = About <fib type="int" name="fib1"/></p>
    <solutions>
      <solution>
        <cond><fib_ref name="fib1"/>==$Math.round(A/10)*10+Math.round(B/10)*10$</cond>
      </solution>
    </solutions>
    <tutelage_ref name="TU1082">
      <bind name="A" val="A"/>
      <bind name="B" val="B"/>
    </tutelage_ref>
  </group>
</problem_tmpl>

