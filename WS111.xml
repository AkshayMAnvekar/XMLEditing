<problem_tmpl name="PR1081" width="300">
  <params>
    <param name="A" type="int"/>
    <param name="B" type="int"/>
    <constraints>
      <cond>1 &lt; A &lt; 5</cond>
      <cond>2 &lt; B &lt; 6</cond>
    </constraints>
  </params>
  <p>Round each number to the nearest ten to estimate the sum:</p>
  <group name="PR1081" type="MCQ">
    <p>$(A*10)+9$ + $(B*10)+9$ = About _____</p>
    <choice name="C1">$(A*10+9)+(B*10+9)$</choice>
    <choice name="C2">$(A+B)*10$</choice>
    <choice name="C3">$(A+1)*10+(B+1)*10$</choice>
    <choice name="C4">$(A+1)*10+B*10$</choice>
    <solutions>
      <solution>
        <div>
          <repeat index="i" val="4">
            <cond><choice_ref name="C$i+1$"/>==$(i)==(2)$</cond>
          </repeat>
        </div>
      </solution>
    </solutions>
    <tutelage_ref name="TU1081M"></tutelage_ref>
  </group>
  <group name="PR1081" type="FIB">
    <p>$(A*10)+9$ + $(B*10)+9$ = About <fib type="int" name="fib1"/></p>
    <solutions>
      <solution>
        <cond><fib_ref name="fib1"/>==$(A+1)*10+(B+1)*10$</cond>
      </solution>
    </solutions>
    <tutelage_ref name="TU1081">
      <bind name="A" val="A"/>
      <bind name="B" val="B"/>
    </tutelage_ref>
  </group>
</problem_tmpl>

<problem_tmpl name="PR1082" width="300">
