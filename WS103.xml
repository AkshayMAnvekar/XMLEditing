        <repeat index="i" val="4">
          <cond>
            <choice_ref name="C$i+1$"/>==$(i)==(1)$
          </cond>
        </repeat>
      </solution>
    </solutions>
    <tutelage_ref name="TU1022M"></tutelage_ref>
  </group>
</problem_tmpl>
