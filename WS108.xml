<problem_tmpl name="PR1057" width="300">
  <params>
    <param name="A" type="int"/>
    <param name="B" type="int"/>
    <constraints>
      <cond>2 &#60;&#61; A &#60;&#61; 5</cond>
      <cond>5 &#60; B &#60; 9</cond>
    </constraints>
  </params>
  <p>Joseph bought fruits and vegetables for $$$A*10+B$. Round the amount he spent to the nearest ten.</p>
  <group name="PR1057" type="FIB">
    <p>He spent about $$<fib type="int" name="fib1"/></p>
    <solutions>
      <solution>
        <cond><fib_ref name="fib1"/>==$(A+1)*10$</cond>
      </solution>
    </solutions>
    <tutelage_ref name="TU1057">
      <bind name="A" val="A"/>
      <bind name="B" val="B"/>
    </tutelage_ref>
  </group>
  <group name="PR1057" type="MCQ">
    <choice name="C1">$$$(A+1)*10$</choice>
    <choice name="C2">$$$A*10$</choice>
    <choice name="C3">$$$A*10+B$</choice>
    <solutions>
      <solution>
        <repeat val="3" index="i">
          <cond><choice_ref name="C$i+1$"/>==$(i)==(0)$</cond>
        </repeat>
      </solution>
    </solutions>
    <tutelage_ref name="TU1057M"></tutelage_ref>
  </group>
</problem_tmpl>
