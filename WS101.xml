<tutelage_tmpl name="TU1001">
	<params>
		<param name="A" type="int"/>
		<param name="B" type="int"/>
		<param name="C" type="int"/>
	</params>
	<feedback name="FB1">
		<trigger>
			<cond><fib_ref name="fib1"/>==$B*100+A*10+C$</cond>
		</trigger>
	</feedback>
	<feedback name="FB2">
		<trigger>
			<cond><fib_ref name="fib1"/>==$C*100+B*10+A$</cond>
		</trigger>
	</feedback>
	<feedback name="FB3">
		<trigger>
			<cond>(Math.floor(<fib_ref name="fib1"/>/100)==Math.floor(<fib_ref name="fib1"/>/10)%10) ||
			(<fib_ref name="fib1"/>%10==Math.floor(<fib_ref name="fib1"/>/10)%10) ||
			(Math.floor(<fib_ref name="fib1"/>/100)==<fib_ref name="fib1"/>%10)</cond>
		</trigger>
	</feedback>
	<feedback name="FB4">
		<trigger>
			<cond><fib_ref name="fib1"/>==$A+B+C$</cond>
		</trigger>
	</feedback>
	<feedback name="FB5">
		<trigger>
			<cond><fib_ref name="fib1"/>==$A*100*1000+B*10*10+C$</cond>
		</trigger>
	</feedback>
	<feedback name="_UNCOMMON">
	</feedback>
</tutelage_tmpl>

