<problem_tmpl name="PR1001" width="300">
	<params>
		<param name="A" type="int"/>
		<param name="B" type="int"/>
		<param name="C" type="int"/>
		<constraints>
			<cond>1 &#8804; A &#8804; 9
			</cond>
			<cond>1 &#8804; B &#8804; 9
			</cond>
			<cond>1 &#8804; C &#8804; 4
			</cond>
			<cond>A &#8800; B
			</cond>
			<cond>B &#8800; C
			</cond>
			<cond>A &#8800; C
			</cond>
		</constraints>
	</params>
	<group name="PR1001" type="FIB">
		<p>$A$ hundreds, $B$ tens, and $C$ ones can be written in standard form as <fib type="int" name="fib1"/></p>
		<solutions>
			<solution>
				<cond>
					<fib_ref name="fib1"/>==$(A*100)+(B*10)+C$
				</cond>
			</solution>
		</solutions>
		<tutelage_ref name="TU1001">
			<bind name="A" val="A"/>
			<bind name="B" val="B"/>
			<bind name="C" val="C"/>
		</tutelage_ref>
	</group>
	<group name="PR1001" type="MCQ">
		<p>$A$ hundreds, $B$ tens, and $C$ ones can be written in standard form as ________.<br/></p>
		<choice name="C1">$(A*100)+(B*10)+C$
		</choice>
		<choice name="C2">$(B*100)+(A*10)+C$
		</choice>
		<choice name="C3">$(A*100)+(C*10)+A$
		</choice>
		<choice name="C4">$(C*100)+(B*10)+A$
		</choice>
		<solutions>
			<solution>
				<repeat index="i" val="4">
					<cond>
						<choice_ref name="C$i+1$"/>==$(i)==(0)$
					</cond>
				</repeat>
			</solution>
		</solutions>
		<tutelage_ref name="TU1001M"></tutelage_ref>
	</group>
</problem_tmpl>

