<problem_tmpl name="PR1049" width="300">
	<params>
		<param name="A" type="int"/>
		<constraints>
			<cond>1 &#60; A &#60; 9</cond>
		</constraints>
	</params>
	<group name="PR1049" type="FIB">
		<p>Round $(A*10)+1$ to the nearest ten.</p>
		<p> <fib name="fib1" /></p>
		<solutions>
			<solution>
				<cond><fib_ref name="fib1" />==$A*10$</cond>
			</solution>
		</solutions>
		<tutelage_ref name="TU1049">
			<bind name="A" val="A"/>
		</tutelage_ref>
	</group>
	<group name="PR1049" type="MCQ">
		<p>Round $(A*10)+1$ to the nearest ten.</p>
		<choice name="C1">$(A+1)*10$</choice>
		<choice name="C2">$(A*10)+1$</choice>
		<choice name="C3">$A*10$</choice>
		<choice name="C4">$(A-1)*10$</choice>
		<solutions>
			<solution>
				<repeat val="4" index="i">
					<cond><choice_ref name="C$i+1$"/>==$(i)==(2)$</cond>
				</repeat>
			</solution>
		</solutions>
		<tutelage_ref name="TU1049M"></tutelage_ref>
	</group>
</problem_tmpl>
<problem_tmpl name="PR1050" width="300">
